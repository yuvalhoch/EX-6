    <Text fx:id="player1text"/>
    <Text fx:id="p1Score" fill="#0000b8" scaleX="1.7085232871879064" scaleY="1.437515699412554" text="" />
    <Text fx:id="player2text"/>
    <Text fx:id="p2Score" />
