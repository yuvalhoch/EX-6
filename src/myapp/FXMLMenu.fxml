<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>


<GridPane xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="myapp.MenuController" alignment="center"
          hgap="10" vgap="10">
    <Text text="Welcome To Reversi's Main Menu"
          GridPane.rowIndex="0" GridPane.columnIndex="0"
          GridPane.columnSpan="2"/>
    <children>
        <Button fx:id="btnStart" GridPane.rowIndex="1" GridPane.columnIndex="2" onAction="#startBtnClicked" text="Start Game" />
        <Button fx:id="btnSetting" GridPane.rowIndex="2" GridPane.columnIndex="2" onAction="#settingsBtnClicked" text="Settings" />
    </children>
</GridPane>



















